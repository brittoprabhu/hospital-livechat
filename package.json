{
  "name": "hospital-livechat",
  "version": "1.0.0",
  "description": "Hospital live chat support system with admin, agent, and widget frontend",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate:up": "node-pg-migrate -m src/db/migrations up",
    "migrate:down": "node-pg-migrate -m src/db/migrations down",
    "migrate:list": "node-pg-migrate -m src/db/migrations list",
     "seed": "node --no-warnings --experimental-json-modules scripts/seedData.js",
 "seed:intel": "node --no-warnings --experimental-json-modules scripts/seedIntelligenceData.js",
 "seed:faqtree": "node --no-warnings --experimental-json-modules scripts/seedFaqTree.js",
 "seed:faqdepartments": "node --no-warnings --experimental-json-modules scripts/seedAssign-Faq-Departments.js"
  },
  "keywords": [
    "chat",
    "socket.io",
    "express",
    "postgresql",
    "hospital",
    "support",
    "widget"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "socket.io": "^4.7.5",
        "winston": "^3.x.x"
  },
    
  "devDependencies": {
    "node-pg-migrate": "8.0.3",
    "nodemon": "^3.1.0"
  }
}
